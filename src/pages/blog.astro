---
import Overview from '../components/blog/Overview.astro';

import { getBlogPosts } from '../utils/storyblok';

const { data, total } = await getBlogPosts(1, import.meta.env.BLOG_ITEMS_PER_PAGE);
---
<Overview page={1} total={Math.ceil(total / import.meta.env.BLOG_ITEMS_PER_PAGE)} stories={data.stories} />
